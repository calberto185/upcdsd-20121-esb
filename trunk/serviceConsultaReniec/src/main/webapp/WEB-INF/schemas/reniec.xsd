<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://www.springsource.com/reniec"
		elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:reniec="http://www.springsource.com/reniec">

	<element name="dniRequest">
		<complexType>
			<sequence>
				<element name="dni" type="reniec:DniType"></element>
			</sequence>
		</complexType>
	</element>

	<complexType name="DniType">
		<attribute name="dni" type="string"/>
	</complexType>

	<element name="dniResponse">
		<complexType>
			<sequence>
				<element name="respuesta" type="reniec:RespuestaType"/>
			</sequence>
		</complexType>
	</element>

	<complexType name="RespuestaType">

		<attribute name="dni" type="string"></attribute>
		<attribute name="nombre" type="string" />
		<attribute name="apellidoPaterno" type="string" />

		<attribute name="apellidoMaterno" type="string" />

		<attribute name="fechaNacimiento" type="string"></attribute>
		<attribute name="direccion" type="string"></attribute>
		<attribute name="estadoCivil" type="string"></attribute>
	</complexType>


</schema>